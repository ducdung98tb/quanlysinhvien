<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>LoginServiceImpl</servlet-name>
    <servlet-class>com.hello.server.LoginServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LoginServiceImpl</servlet-name>
    <url-pattern>/hello/service/loginservice</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
    <servlet-name>RootServiceImpl</servlet-name>
    <servlet-class>com.hello.server.RootServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>RootServiceImpl</servlet-name>
    <url-pattern>/hello/service/getListStudent</url-pattern>
  </servlet-mapping>
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Hello.html</welcome-file>
  </welcome-file-list>
  

</web-app>
